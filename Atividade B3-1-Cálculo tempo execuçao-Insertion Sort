#include <stdio.h>
void insertionSort(int arr[], int n) {
    int j, key, i;
    for (j = 1; j < n; j++) {
       key = arr[j];  
        i = j - 1;     
        while (i >= 0 && arr[i] > key) {         
            arr[i + 1] = arr[i]; 
            i = i - 1;   
        }
        arr[i + 1] = key; 
    }
}
void printArray(int arr[], int size) {
    for (int i = 0; i < size; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");
}
int main() {
    int arr[] = {12, 11, 13, 5, 6};
    int n = sizeof(arr) / sizeof(arr[0]);

    printf("Array original:\n");
    printArray(arr, n);

    insertionSort(arr, n);

    printf("Array ordenado:\n");
    printArray(arr, n);
    return 0;
}


CONTAGEM DE t

