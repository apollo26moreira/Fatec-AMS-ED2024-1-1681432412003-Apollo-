2. int count = 0; — 1 t
3. for (j = 1; j < n; j++) { — 2t
4. key = arr[j]; — 1 acesso, 1t
5. i = j - 1; — 1 subtração, 1t
6. while (i >= 0 && arr[i] > key) {  (i >= 0), (arr[i] > key) 3t
7. arr[i + 1] = arr[i]; — 2t
8. i = i - 1; — 2t
9. arr[i + 1] = key; — 2t
10. printf("Total de operações: %d\n", count); — 2t
